{
  "$schema": "https://unpkg.com/@changesets/config@3.1.1/schema.json",
  "changelog": "@changesets/cli/changelog",
  "commit": [
    "@changesets/cli/commit",
    {
      "skipCI": false
    }
  ],
  "fixed": [],
  "linked": [
    [
      "@gram-ai/functions",
      "@gram-ai/create-function"
    ]
  ],
  "ignore": [
    "gram-template-*"
  ],
  "access": "restricted",
  "baseBranch": "origin/main",
  "updateInternalDependencies": "patch",
  "privatePackages": {
    "version": true,
    "tag": true
  }
}