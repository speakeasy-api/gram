{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/index.ts"],
  "readme": "./README.typedoc.md",
  "mergeReadme": false,
  "out": "docs",
  "plugin": ["typedoc-plugin-markdown"],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeExternals": true,
  "disableSources": true,
  "name": "@gram-ai/elements",
  "includeVersion": true,
  "skipErrorChecking": false,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },

  "sort": ["source-order"],
  "kindSortOrder": [
    "Function",
    "Class",
    "Interface",
    "TypeAlias",
    "Variable",
    "Enum"
  ],
  "treatWarningsAsErrors": false,
  "tsconfig": "./tsconfig.json"
}
