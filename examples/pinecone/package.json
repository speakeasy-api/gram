{
  "type": "module",
  "name": "pinecone",
  "version": "0.0.0",
  "author": "",
  "description": "",
  "private": true,
  "keywords": [
    "mcp",
    "getgram.ai"
  ],
  "license": "ISC",
  "main": "dist/functions.js",
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=22.18.0"
  },
  "scripts": {
    "dev": "npm run db:start && mcp-inspector nodemon -q --watch src --exec node ./src/server.ts",
    "lint": "tsc --noEmit",
    "push": "gf push",
    "prebuild": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "build": "gf build",
    "db:start": "docker compose up -d && npm run db:wait && npm run db:seed",
    "db:stop": "docker compose down",
    "db:reset": "docker compose down && npm run db:start",
    "db:wait": "for i in {1..30}; do curl -s http://localhost:5081/ > /dev/null 2>&1 && break || sleep 1; done",
    "db:seed": "node --import tsx scripts/seed.ts"
  },
  "dependencies": {
    "@gram-ai/functions": "^0.11.1",
    "@modelcontextprotocol/sdk": "^1.20.1",
    "@openrouter/sdk": "^0.1.27",
    "@pinecone-database/pinecone": "^6.1.3",
    "papaparse": "^5.5.3",
    "zod": "^4"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.17.1",
    "@types/node": "22.x",
    "@types/papaparse": "^5.5.0",
    "nodemon": "^3.1.9",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}